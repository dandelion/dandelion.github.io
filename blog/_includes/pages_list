      	{% else %}
      	<li><a href="{{ BASE_PATH }}{{node.url}}">{{node.title}}{% if node.tagline %} {{node.tagline}}{% endif %}</a></li>
      	{% endif %}
      {% endif %}
    {% endif %}
  {% endfor %}
